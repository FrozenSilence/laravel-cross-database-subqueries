{
    "name": "frozensilence/laravel-cross-database-subqueries",
    "description": "Eloquent cross database compatibility in subqueries",
    "type": "library",
    "require": {
        "php": ">=8.3",
        "illuminate/support": "^12.0",
        "illuminate/container": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/events": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "FrozenSilence\\CrossDatabase\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FrozenSilence\\Tests\\" : "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FrozenSilence\\CrossDatabase\\CrossDatabaseServiceProvider"
            ]
        }
    }
}
